{
  "name": "fp-omit",
  "version": "0.1.0",
  "description": "a functional method to omit n level property from an object",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --exit --recursive ./test/**/*.spec.js",
    "coverage": "nyc npm run test",
    "lint": "eslint .",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags"
  },
  "keywords": [
    "fp-omit",
    "omit",
    "remove",
    "immutable",
    "functional",
    "utils",
    "fp",
    "dissocpath"
  ],
  "author": " <ahmar siddiqui>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahhmarr/fp-omit.git"
  },
  "bugs": {
    "url": "https://github.com/ahhmarr/fp-omit/issues"
  },
  "homepage": "https://github.com/ahhmarr/fp-omit#readme",
  "dependencies": {
    "ramda.dissocpath": "0.26.1"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.20.1",
    "husky": "4.2.3",
    "mocha": "7.1.1",
    "nyc": "15.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  }
}
